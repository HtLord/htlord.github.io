{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "baseUrl": "src",
    "jsx": "react-jsx",
    "paths": {
      "@assets/*": ["assets/*"],
      "@consts": ["./consts.ts"],
      "@components/*": ["components/*"],
      "@content/*": ["content/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"],
      "@styles/*": ["styles/*"],
      "@libs/*": ["libs/*"]
    }
  },
}