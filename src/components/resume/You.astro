---
import '@styles/tailwind.css';
import { getEntry } from "astro:content";
import Contacts from "./Contacts.astro";

const resumeData =  await getEntry('resume', 'index');
---
<div
    class="
      flex justify-between
    "
>
  <div
      class="flex flex-col"
  >
    <h3>{ resumeData.data.you.name }</h3>
    <h4 class="text-gray">{ resumeData.data.you.title }</h4>
  </div>
  <div
      class="flex"
  >
    <Contacts />
  </div>
</div>